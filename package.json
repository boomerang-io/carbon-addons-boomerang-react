{
  "name": "@boomerang-io/carbon-addons-boomerang-react",
  "version": "3.0.0-beta.17",
  "license": "Apache-2",
  "repository": {
    "type": "git",
    "url": "git@github.com:boomerang-io/carbon-addons-boomerang-react"
  },
  "homepage": "https://carbon-addons-boomerang-react.netlify.app",
  "bugs": {
    "url": "https://github.com/boomerang-io/carbon-addons-boomerang-react/issues"
  },
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "lib/**/*",
    "es/**/*",
    "scss/**/*"
  ],
  "sideEffects": false,
  "keywords": [
    "react",
    "carbon",
    "components",
    "boomerang",
    "storybook"
  ],
  "scripts": {
    "build": "node scripts/build.js",
    "build:storybook": "build-storybook",
    "lint": "eslint src",
    "prebuild": "rimraf lib styles es docs",
    "prepare": "husky install",
    "precommit": "lint-staged",
    "start": "start-storybook -p 6006",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:staged": "cross-env CI=true vitest related --passWithNoTests",
    "prepublishOnly": "pnpm build",
    "release": "release-it"
  },
  "dependencies": {
    "@babel/runtime": "7.18.9",
    "@stomp/stompjs": "6.1.2",
    "@tippyjs/react": "4.2.6",
    "classnames": "2.3.1",
    "date-fns": "2.29.1",
    "dompurify": "2.3.10",
    "downshift": "6.1.7",
    "focus-trap-react": "9.0.2",
    "lodash.get": "4.4.2",
    "lodash.isequal": "4.5.0",
    "lodash.sortby": "4.7.0",
    "match-sorter": "6.3.1",
    "react-autosuggest": "10.1.0",
    "react-modal": "3.15.1",
    "react-toastify": "9.0.7"
  },
  "peerDependencies": {
    "@carbon/react": "^1.12.0",
    "axios": ">=0.26.0",
    "formik": "^2.1.5",
    "prop-types": "^15.8.1",
    "react": "^17.0.0 || ^16.14.0",
    "react-dom": "^17.0.0 || ^16.14.0",
    "react-query": "3.34.12",
    "react-router-dom": "^5.3.0",
    "yup": ">=0.32.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.6",
    "@babel/core": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@carbon/react": "1.12.0",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@storybook/addon-a11y": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/addon-storysource": "^6.5.12",
    "@storybook/builder-vite": "^0.2.2",
    "@storybook/react": "^6.5.12",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@vitejs/plugin-react": "^2.1.0",
    "axios": "0.26.0",
    "axios-mock-adapter": "^1.20.0",
    "babel-loader": "^8.2.5",
    "core-js": "^3.23.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.19.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "formik": "^2.2.9",
    "husky": "^8.0.1",
    "in-publish": "^2.0.1",
    "jest-axe": "^6.0.0",
    "jsdom": "^20.0.0",
    "lint-staged": "^13.0.3",
    "npm-which": "^3.0.1",
    "prop-types": "^15.8.1",
    "react": "^16.14.0",
    "react-dom": "16.14.0",
    "react-query": "3.34.12",
    "react-router-dom": "^5.3.0",
    "release-it": "^15.4.2",
    "rimraf": "^3.0.2",
    "sass": "^1.53.0",
    "storybook": "^6.5.12",
    "typescript": "^4.6.3",
    "vite": "^3.1.3",
    "vitest": "0.23.4",
    "yup": "^0.32.11"
  },
  "lint-staged": {
    "src/**/*": "prettier --write --ignore-unknown",
    "src/**/*.{js,jsx,ts,tsx}": [
      "pnpm lint",
      "pnpm test:staged"
    ]
  },
  "browserslist": {
    "production": [
      "defaults"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "release-it": {
    "github": {
      "release": true
    },
    "git": {
      "commitMessage": "chore: release v${version}",
      "requireCleanWorkingDir": false
    }
  }
}
