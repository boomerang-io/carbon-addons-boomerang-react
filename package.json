{
  "name": "@boomerang-io/carbon-addons-boomerang-react",
  "description": "Carbon Addons for Boomerang apps",
  "version": "1.5.0-beta.5",
  "author": {
    "name": "Tim Bula",
    "email": "timrbula@gmail.com"
  },
  "license": "Apache-2",
  "repository": {
    "type": "git",
    "url": "git@github.com:boomerang-io/carbon-addons-boomerang-react"
  },
  "homepage": "https://carbon-addons-boomerang-react.netlify.app",
  "bugs": {
    "url": "https://github.com/boomerang-io/carbon-addons-boomerang-react/issues"
  },
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "lib/**/*",
    "es/**/*",
    "styles/**/*"
  ],
  "keywords": [
    "react",
    "carbon",
    "components",
    "boomerang",
    "storybook"
  ],
  "scripts": {
    "build": "yarn build:pre && node scripts/build.js",
    "build:pre": "rimraf lib styles es docs",
    "build:storybook": "build-storybook -s public/production -c .storybook -o docs",
    "format": "prettier --write \"**/*.{scss,css,js,jsx,md,ts}\"",
    "format:diff": "prettier --list-different \"**/*.{scss,css,js,jsx,md,ts}\"",
    "lint": "yarn lint:javascript && yarn lint:stylelint",
    "lint:javascript": "eslint 'src/**/*.{js,jsx}'",
    "lint:stylelint": "stylelint './src/**/*.scss' --syntax scss --ignorePath .gitignore",
    "precommit": "lint-staged",
    "prepublishOnly": "yarn build",
    "publish-npm": "yarn semantic-release",
    "start": "yarn --max_old_space_size=16384 storybook",
    "storybook": "start-storybook -p 3000 -s public/development",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage",
    "test:staged": "cross-env CI=true react-scripts test --passWithNoTests --findRelatedTests --bail"
  },
  "dependencies": {
    "@babel/runtime": "^7.16.7",
    "@carbon/themes": "^10.49.0",
    "@stomp/stompjs": "^5.4.2",
    "@tippyjs/react": "^4.0.2",
    "classnames": "^2.2.5",
    "date-fns": "^1.30.1",
    "dompurify": "^2.0.6",
    "downshift": "^5.2.1",
    "focus-trap-react": "^8.9.1",
    "js-file-download": "^0.4.7",
    "match-sorter": "^6.3.1",
    "react-autosuggest": "^9.4.3",
    "react-dropzone": "^10.0.6",
    "react-modal": "^3.10.1",
    "react-toastify": "^5.3.2",
    "sockjs-client": "^1.5.2",
    "window-or-global": "^1.0.1"
  },
  "peerDependencies": {
    "carbon-components": "^10.25.0",
    "carbon-components-react": "^7.25.0",
    "carbon-icons": "^7.0.7",
    "formik": "^2.1.5",
    "lodash": " 4.17.21",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-router-dom": "^5.2.0",
    "yup": "^0.32.11"
  },
  "peerDependenciesMeta": {
    "formik": {
      "optional": true
    },
    "lodash": {
      "optional": true
    },
    "react-router-dom": {
      "optional": true
    },
    "yup": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/plugin-transform-runtime": "^7.16.10",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@boomerang-io/carbon-addons-boomerang-react": "1.4.1",
    "@rollup/plugin-babel": "^5.3.0    ",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3    ",
    "@storybook/addon-a11y": "^6.4.14",
    "@storybook/addon-actions": "^6.4.14",
    "@storybook/addon-centered": "^5.2.8",
    "@storybook/addon-controls": "^6.4.14",
    "@storybook/addon-info": "^5.3.18",
    "@storybook/addon-knobs": "^6.4.0",
    "@storybook/addon-links": "^6.4.14",
    "@storybook/addon-options": "^5.3.9",
    "@storybook/addon-storyshots": "^6.4.14",
    "@storybook/addon-storysource": "^6.4.14",
    "@storybook/addons": "^6.4.14",
    "@storybook/builder-webpack5": "^6.4.14",
    "@storybook/manager-webpack5": "^6.4.14",
    "@storybook/preset-create-react-app": "^4.0.0",
    "@storybook/react": "^6.4.14",
    "@storybook/theming": "^6.4.14",
    "@testing-library/dom": "^8.11.2",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "12.1.2",
    "autoprefixer": "^10.4.2",
    "axios": "^0.25.0",
    "axios-mock-adapter": "^1.20.0",
    "babel-plugin-dev-expression": "^0.2.3",
    "browserslist-config-carbon": "^10.6.1",
    "carbon-components": "10.25.0",
    "carbon-components-react": "7.25.0",
    "carbon-icons": "^7.0.7",
    "cross-env": "^7.0.3",
    "formik": "^2.1.5",
    "in-publish": "^2.0.1",
    "jest-axe": "^5.0.1",
    "lint-staged": "^12.3.1",
    "lodash": " 4.17.21",
    "match-sorter": "^6.3.1",
    "mock-socket": "^9.0.3",
    "moment": "^2.29.1",
    "npm-run-all": "^4.1.5",
    "npm-which": "^3.0.1",
    "postcss": "^8.4.5",
    "prettier": "2.5.1",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-helmet": "^6.0.0",
    "react-query": "3.34.12",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^5.0.0",
    "react-test-renderer": "~16",
    "rimraf": "^3.0.2",
    "rollup": "^2.64.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-filesize": "^9.1.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-summary": "^1.3.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.49.0",
    "semantic-release": "^19.0.2",
    "storybook-addon-rtl": "^0.4.2",
    "storybook-readme": "^5.0.5",
    "stylelint": "^14.3.0",
    "stylelint-config-recommended-scss": "^5.0.2",
    "stylelint-scss": "^4.1.0",
    "webpack": "^5.66.0",
    "yup": "^0.32.11"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      "last 1 version",
      "> 1%",
      "ie 11"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "npm run-script lint",
      "npm run-script test:staged"
    ],
    "*.{scss,css}": [
      "yarn lint:stylelint"
    ],
    "*.{scss,css,js,md,jsx,json}": [
      "yarn format",
      "git add"
    ]
  },
  "sideEffects": false,
  "stylelint": {
    "extends": "stylelint-config-recommended-scss",
    "plugins": [
      "stylelint-scss"
    ],
    "ignoreFiles": [
      "**/*.js"
    ]
  },
  "babel": {
    "presets": [
      "./scripts/env"
    ]
  }
}
